    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner" role="listbox">
    <div class="item active">
      <!-- http://www.wallmoy.com/wp-content/uploads/2015/02/new_york_city_wallpaper_good.jpg -->
      <img src="icons/nyc_carousel.jpg" alt="First slide">
      <div class="container">
        <div class="carousel-caption">
          <p><a class="btn btn-lg btn-info" href="/nyc" role="button">Rides to NYC</a></p>
        </div>
      </div>
    </div>
    <div class="item">
      <!-- http://www.mrwallpaper.com/wallpapers/airplane-flight-sunset-1920x1080.jpg -->	
